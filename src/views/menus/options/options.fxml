<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="optionsMenu" alignment="CENTER" onKeyReleased="#keyHandler" visible="false" xmlns="http://javafx.com/javafx/8.0.65"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.menus.Options">
    <children>
        <Button id="graphics" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onMouseClicked="#mouseHandler" text="GRAPHICS">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Button id="audio" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onMouseClicked="#mouseHandler" text="AUDIO">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Button id="input" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onMouseClicked="#mouseHandler" text="INPUT">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Button id="credits" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onMouseClicked="#mouseHandler" text="CREDITS">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Button id="back" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onMouseClicked="#mouseHandler" text="BACK">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
    </children>

    <stylesheets>
        <URL value="@options.css" />
    </stylesheets>
</VBox>
