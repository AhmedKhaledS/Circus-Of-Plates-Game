<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<!-- options menu -->
<VBox fx:id="optionsMenu" alignment="TOP_CENTER" layoutX="236.0" layoutY="149.0" prefHeight="230.0" prefWidth="228.0" visible="false"
      fx:controller="controllers.menus.Options" xmlns:fx="http://javafx.com/fxml/1"
      onKeyTyped="#handleKeyAction" onKeyPressed="#keyHandler">
    <children>
        <Button id="graphics" mnemonicParsing="false"  prefHeight="35.0" prefWidth="232.0" text="GRAPHICS">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Button id="audio" mnemonicParsing="false"  prefHeight="35.0" prefWidth="232.0" text="AUDIO">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Button id="input" mnemonicParsing="false"  prefHeight="24.0" prefWidth="232.0" text="INPUT">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Button id="credits" mnemonicParsing="false"  prefHeight="24.0" prefWidth="232.0" text="CREDITS">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Button id="back" mnemonicParsing="false"  prefHeight="24.0" prefWidth="232.0" text="BACK">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
    </children>

    <stylesheets>
        <URL value="@options.css" />
    </stylesheets>
</VBox>