<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>

<SplitPane fx:id="audioSettings" dividerPositions="0.5" orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0"
           fx:controller="controllers.menus.options.Audio" xmlns:fx="http://javafx.com/fxml/1">
    <items>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
            <children>
                <Slider fx:id="volumeSlider" layoutX="14.0" layoutY="48.0" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="16.0" prefWidth="185.0" value="50.0" />
                <Label layoutX="14.0" layoutY="31.0" prefHeight="17.0" prefWidth="69.0" text="volume" textAlignment="CENTER" />
            </children>
        </AnchorPane>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
            <children>
                <RadioButton fx:id="muteOption" layoutX="14.0" layoutY="30.0" mnemonicParsing="false" text="Mute" />
            </children>
        </AnchorPane>
    </items>
</SplitPane>